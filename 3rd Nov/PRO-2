//Q2) Write a program to display the following pattern
1
10
101
1010
10101  
//


package shweta;

public class pattern {
	public static void main(String [] args)
	{
		
	
	
int i,j;
for(i=1;i<=5;i++)//row
{
	for(j=1;j<=i;j++)  //col
		{
		if(j%2==0) {
			System.out.print(0);
		}
		else {
			System.out.print(1);

		}
	}
	System.out.println();
}
}


